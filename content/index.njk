{% set list = [] %}

{# Build a filtered list manually — Nunjucks-compatible #}
{% for p in collections.posts | reverse %}
  {% if not p.data.draft %}
    {% set list = list.concat([p]) %}
  {% endif %}
{% endfor %}

{# Now show only first 10 #}
<b>Published recently</b>
<ul>
{% for post in list | head(10) %}
  <li>
    <a href="{{ post.url }}">{{ post.data.title }}</a>
    <time datetime="{{ post.date | htmlDateString }}">
      / {{ post.date | readableDate("dd LLL yyyy") }}
    </time>
  </li>
{% endfor %}
</ul>
